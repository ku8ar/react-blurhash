{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "nodenext",
    "target": "esnext",
    "lib": ["webworker", "es6", "dom"],
    "declaration": true,
    "outDir": "./lib",
    "sourceMap": true,
    "alwaysStrict": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "jsx": "react",
    "allowJs": true
  },
  "include": ["src"]
}
